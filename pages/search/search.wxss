/* pages/searchData/searchData.wxss */
.dataFormat{
  flex-direction: column ;
  justify-content: center;
  align-items:center;
  align-content:center ;
  font-size: 30rpx;
  color: green;
}
.searchInfo{
  height:90rpx; width: 40%; display:flex; justify-content: center;align-items:center;
}
.dateInfo{
  text-align: center;
}
.history-table-wrap{
  overflow-y: scroll;
  overflow-x: hidden;
}
.table{
  border:1px solid #dadada;
  /* border-right:0; */
  /* border-bottom: 0; */
  /* width: 100%; */
  /* width: 98%; */
  /* margin-left: 1%; */
}
.tr{
  width:100%;
  display: flex;
  justify-content: space-between;
}
.th,.td{
  padding: 10px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
}
.th1,.th2,.td1,.td2{
  width: 100%;
}
.th{
  font-weight: 800;
  background-color: #0a4d20;
  font-size: 28rpx;
  color: #bafad5;
}
.td{
  font-size: 20rpx;
  color: #1aa521;
  text-align: left;
}
